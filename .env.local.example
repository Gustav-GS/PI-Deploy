# Postgres connection string (Vercel Postgres exposes POSTGRES_URL)
POSTGRES_URL="postgres://user:password@host:5432/db"
# Optional: sometimes libraries use DATABASE_URL
DATABASE_URL="${POSTGRES_URL}"

# JWT secret for admin sessions
JWT_SECRET="change-me-to-a-long-random-string"

# Vercel Blob read/write token (Project Settings -> Storage)
BLOB_READ_WRITE_TOKEN="vercel_blob_rw_token_here"

# For SSR fetches during dev, set base URL if needed
# NEXT_PUBLIC_BASE_URL="http://localhost:3000"

# Optional: contact for geocoder User-Agent (email or URL)
GEOCODER_CONTACT="seu-email@dominio.com"
